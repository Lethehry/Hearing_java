<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_messages">

    <fragment
        android:id="@+id/navigation_keywords"
        android:name="com.example.hearing_java_figma.keywordsFragment"
        android:label="@string/title_keywords"
        tools:layout="@layout/fragment_keywords" />


    <fragment
        android:id="@+id/navigation_messages"
        android:name="com.example.hearing_java_figma.messagesFragment"
        android:label="@string/title_messages"
        tools:layout="@layout/fragment_messages" />

    <fragment
        android:id="@+id/navigation_devices"
        android:name="com.example.hearing_java_figma.devicesFragment"
        android:label="@string/title_devices"
        tools:layout="@layout/fragment_devices" />
